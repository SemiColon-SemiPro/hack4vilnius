Table "applications" {
  "id" INTEGER [pk]
  "applicant_id" INTEGER [not null]
  "type_id" INTEGER
  "score" INTEGER
  "status" TEXT
  "created_at" TEXT
  "updated_at" TEXT
}

Table "applicants" {
  "id" INTEGER [pk]
  "status" TEXT
  "first_name" TEXT [not null]
  "middle_name" TEXT
  "last_name" TEXT [not null]
  "age" INTEGER [not null]
  "address_id" INTEGER
  "type_id" INTEGER
  "created_at" TEXT
  "updated_at" TEXT
}

Table "houses" {
  "id" INTEGER [pk]
  "available" INTEGER [not null]
  "district" TEXT [not null]
  "street" TEXT [unique, not null]
  "house_number" INTEGER
  "flat_number" TEXT
  "useful_mq" TEXT [not null]
  "category" TEXT  
  "capacity" INTEGER [not null]
  "floor" INTEGER [not null]
  "rooms_number" INTEGER [not null]
  "elevator" INTEGER [not null] 
  "assigned_to" INTEGER 
  "created_at" TEXT
  "updated_at" TEXT
}

Table "addresses" {
  "id" INTEGER [pk]

  "created_at" TEXT
  "updated_at" TEXT
}

Table "type"{
  "id" INTEGER [pk]
  "family_size" INTEGER
  "disability" INTEGER
  "refugee" INTEGER
  "finacial_bracket" INTEGER
  "priority_level" INTEGER
}

Table "attributes"{
  "id" INTEGER [pk]
  "property_owner" INTEGER
  "finacial" TEXT [not null]
  "declartion_of_assets" TEXT
}


Ref:"applicants"."id" < "applications"."applicant_id"

Ref:"addresses"."id" < "applicants"."address_id"

Ref:"applicants"."id" < "houses"."assigned_to"

Ref:"type"."id" < "applications"."type_id"

Ref:"type"."id" < "applicants"."type_id"